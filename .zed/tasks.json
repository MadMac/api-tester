[
  {
    "label": "Start Frontend Dev Server",
    "command": "npm",
    "args": ["run", "dev"],
    "env": {},
    "cwd": "$ZED_WORKTREE_ROOT",
    "use_new_terminal": true,
    "allow_concurrent_runs": false,
    "reveal": "always"
  },
  {
    "label": "Build Frontend",
    "command": "npm",
    "args": ["run", "build"],
    "env": {},
    "cwd": "$ZED_WORKTREE_ROOT",
    "use_new_terminal": false,
    "allow_concurrent_runs": false,
    "reveal": "always"
  },
  {
    "label": "Clean Build",
    "command": "cargo",
    "args": ["clean"],
    "env": {},
    "cwd": "$ZED_WORKTREE_ROOT/src-tauri",
    "use_new_terminal": false,
    "allow_concurrent_runs": false,
    "reveal": "always"
  },
  {
    "label": "Tauri Dev (Full)",
    "command": "cargo",
    "args": ["tauri", "dev"],
    "env": {},
    "cwd": "$ZED_WORKTREE_ROOT",
    "use_new_terminal": true,
    "allow_concurrent_runs": false,
    "reveal": "always"
  }
]
